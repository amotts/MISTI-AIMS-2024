<launch>
	<!-- Include the MAVROS node with SITL and Gazebo -->
	<include file="$(find px4)/launch/mavros_posix_sitl.launch">
	 <param name="world" value="$(find mavlink_sitl_gazebo)/worlds/custom.world"/>
	</include>

	<node pkg="feedback_control" type="set_offb.py" name="set_offb" output="screen" />

	<node pkg="feedback_control" type="ground_simulator_node.py" name="vehicle_simulator" required="true" output="screen"/>
      <param name="csv_file" value="/home/amotz/Documents/depth_profile.csv"/>
      <param name="speed" value="2.0"/>
	  <param name="pub_rate" value ="3.0"/>

	<node pkg="feedback_control" type="depth_controller_node.py" name="depth_controller" required="true" output="screen"/>
      <param name="initial_depth" value="5.0"/>
	  <param name="desired_alt" value="1.5"/>
	  <param name="depth_var" value="0.5"/>
	  <param name="sensor_var" value="10.0"/>
</launch>